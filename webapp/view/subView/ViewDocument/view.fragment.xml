<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <VBox class="sapUiSmallMargin">
        <f:SimpleForm id="SimpleFormChange480_12120"
            editable="true"
            layout="ResponsiveGridLayout"
            title="Terms and Conditons"
            labelSpanXL="4"
            labelSpanL="4"
            labelSpanM="12"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="0"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="2"
            columnsM="1"
            singleContainerFullSize="false" >
            <f:content>
                <core:Title text="Vendor" />
                <Label text="Vendor Name" />
                <Input value="{VENDOR_NAME}" />
                
                <core:Title text="" />
                <Label text="Vendor Code" />
                <Input value="{VENDOR_CODE}" />
            </f:content>
        </f:SimpleForm>
    </VBox>
    
    <VBox class="sapUiSmallMargin">
        <f:SimpleForm id="SimpleFormChange480_12120-2"
            editable="true"
            layout="ResponsiveGridLayout"
        
            labelSpanXL="4"
            labelSpanL="4"
            labelSpanM="12"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="0"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="2"
            columnsM="1"
            singleContainerFullSize="false">
            <f:content>
                <core:Title text="Document" />
                <Label text="Version" />
                <Input value="{VERSION}" />
                <Label text="Date of Upload" />
                <Input value="{DOCUMENT_UPLOAD DATE}" />
                <Label text="Date of Acceptance" />
                <Input value="{SupplierName}" />
                
                <core:Title text="" />
                <Label text="Uploaded By" />
                <Input value="{VENDOR_NAME}" />
                <Label text="Status" />
                <Input value="{STATUS}" />
            </f:content>
        </f:SimpleForm>
    </VBox>
    
        
    <VBox class="sapUiSmallMargin">
            <f:SimpleForm id="SimpleFormChange480_12120-3"
            editable="true"
            layout="ResponsiveGridLayout"
        
            labelSpanXL="4"
            labelSpanL="4"
            labelSpanM="12"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="0"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="2"
            columnsM="1"
            singleContainerFullSize="false">
            <f:content>
                <core:Title text="Content" />
                <Label text="File" />
                <Input value="{SupplierName}" />
               
                
                <core:Title text="" />
                <Label text="Remarks" />
                <Input value="{SupplierName}" />
                
            </f:content>
        </f:SimpleForm>
    </VBox>     
</core:FragmentDefinition>